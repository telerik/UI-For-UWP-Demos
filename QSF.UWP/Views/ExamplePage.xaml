            </Storyboard>
            <Storyboard x:Name="CloseConfiguratorAnimation">
                <RepositionThemeAnimation FromHorizontalOffset="-155" SpeedRatio="0.5" TargetName="example"/>
                <RepositionThemeAnimation SpeedRatio="0.5" FromHorizontalOffset="-100" TargetName="toolsPlaceholder"/>
            </Storyboard>
            <Storyboard x:Name="CloseConfiguratorOnTop">
                <RepositionThemeAnimation SpeedRatio="0.5" FromHorizontalOffset="-100" TargetName="toolsPlaceholder"/>
            </Storyboard>
            <Storyboard x:Name="NavigationToPreviousAnimation">
                <RepositionThemeAnimation FromHorizontalOffset="-600" SpeedRatio="0.5"  TargetName="exampleViewerPlaceholder"/>
                <RepositionThemeAnimation FromHorizontalOffset="-600" SpeedRatio="0.5"  TargetName="codeViewPlaceholder"/>
            </Storyboard>
            <Storyboard x:Name="NavigationToNextAnimation">
                <RepositionThemeAnimation FromHorizontalOffset="600" SpeedRatio="0.5" TargetName="exampleViewerPlaceholder"/>
                <RepositionThemeAnimation FromHorizontalOffset="600" SpeedRatio="0.5" TargetName="codeViewPlaceholder"/>
            </Storyboard>
            <converters:InvertedBooleanToVisibilityConverter x:Key="InvertedBooleanToVisibilityConverter" />
        </ResourceDictionary>
    </Page.Resources>
