                                        Style="{StaticResource RoundButtonStyle}" 
                                        Click="OnDescriptionFlyoutButtonClick" />
                                <ToggleButton x:Name="configuratorButton" Content="Settings" Click="OnSettingChanged" 
                                                Visibility="{Binding ExampleHasConfigurator, Converter={StaticResource BooleanToVisibilityConverter}}" 
                                                Style="{StaticResource ConfiguratorButtonStyle}" />
                            </StackPanel>
                            <!--Here loads the example-->
                            <ContentControl x:Name="example" Content="{Binding ExampleObject}" Margin="0 0 0 80" Grid.Row="1" Grid.Column="0" HorizontalContentAlignment="Stretch"  />
