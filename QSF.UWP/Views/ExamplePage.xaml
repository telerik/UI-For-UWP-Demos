                <Button x:Name="prevButton" 
                    Content="prev" 
                    Command="{Binding NavigateCommand}" CommandParameter="{Binding PreviousExample}"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Center"
                    Style="{StaticResource NavigationButtonStyle}" />
                <Button x:Name="nextButton" Grid.Column="2"
                    Content="next"
                    Command="{Binding NavigateCommand}" CommandParameter="{Binding NextExample}"
                    HorizontalAlignment="Right"
                    VerticalAlignment="Center"
                    Style="{StaticResource NavigationButtonStyle}" RenderTransformOrigin="0.5,0.5" >
                    <Button.RenderTransform>
                        <CompositeTransform Rotation="180"/>
                    </Button.RenderTransform>
                </Button>
                <Pivot x:Name="rootPivot" Margin="0 28 0 0" Grid.Column="1" Style="{StaticResource QSFHyperlinkPivotStyle}" >
                    <Pivot.Resources>
                        <Style TargetType="PivotHeaderItem" BasedOn="{StaticResource QSFHyperlinkPivotHeaderItemStyle}" />
                    </Pivot.Resources>
                    <PivotItem x:Name="examplePivotItem" Padding="0 28 0 0">
                        <PivotItem.Header>
                            <TextBlock Text="Demo" x:Name="demoHeader"/>
                        </PivotItem.Header>
                        <Grid x:Name="exampleViewerPlaceholder" >
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="*" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>
                            <TextBlock x:Name="exampleHeader" Margin="5 0 0 0"
                                       HorizontalAlignment="Left"
                                       VerticalAlignment="Center"
                                       IsHitTestVisible="False" 
                                       Grid.Row="0"
                                       Grid.ColumnSpan="2"
                                       Text="{Binding Example.HeaderText}" 
                                       Style="{StaticResource GroupHeaderTextStyle}" />
                            <StackPanel x:Name="toolsPlaceholder" Padding="5 0" Grid.ColumnSpan="2" Grid.Row="0" Orientation="Horizontal" HorizontalAlignment="Right" VerticalAlignment="Top">
