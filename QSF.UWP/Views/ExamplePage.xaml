                                          VerticalAlignment="Top" HorizontalAlignment="Stretch"
                                          DisplayMemberPath="FileName"
                                          ItemsSource="{Binding ExampleCodeFilesCollectionViewSource.View}" />
                            </StackPanel>
                            <controls:CodeViewer x:Name="codeViewer" Grid.Row="1" Background="#f4f4f4" BorderThickness="0"  Visibility="{Binding Visibility, RelativeSource={RelativeSource TemplatedParent}}"                                              
                                                 VerticalAlignment="Stretch"  HorizontalAlignment="Stretch" 
                                                 CodeFile="{Binding ExampleCodeFilesCollectionViewSource.View.CurrentItem}"/>
                        </Grid>
                    </PivotItem>
                </Pivot>
            </Grid>
        </Grid>
        <VisualStateManager.VisualStateGroups>
            <!-- Visual states reflect the application's view state -->
            <VisualStateGroup x:Name="ConfiguratorStates">
                <VisualState x:Name="ConfiguratorClosed">
                    <Storyboard>
                        <DoubleAnimationUsingKeyFrames EnableDependentAnimation="True" Storyboard.TargetProperty="(FrameworkElement.Width)" Storyboard.TargetName="configuratorPlaceholder">
                            <EasingDoubleKeyFrame KeyTime="0:0:0" Value="275"/>
                            <EasingDoubleKeyFrame KeyTime="0:0:0" Value="120">
                                <EasingDoubleKeyFrame.EasingFunction>
                                    <CircleEase />
                                </EasingDoubleKeyFrame.EasingFunction>
                            </EasingDoubleKeyFrame>
                        </DoubleAnimationUsingKeyFrames>
                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Visibility)" Storyboard.TargetName="configuratorPlaceholder">
                            <DiscreteObjectKeyFrame KeyTime="0">
                                <DiscreteObjectKeyFrame.Value>
                                    <Visibility>Visible</Visibility>
                                </DiscreteObjectKeyFrame.Value>
                            </DiscreteObjectKeyFrame>
                            <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                <DiscreteObjectKeyFrame.Value>
                                    <Visibility>Collapsed</Visibility>
                                </DiscreteObjectKeyFrame.Value>
                            </DiscreteObjectKeyFrame>
                        </ObjectAnimationUsingKeyFrames>
                    </Storyboard>
                </VisualState>
                <VisualState x:Name="ConfiguratorClosedOnTop">
                    <Storyboard>
                        <DoubleAnimationUsingKeyFrames EnableDependentAnimation="True" Storyboard.TargetProperty="(FrameworkElement.Width)" Storyboard.TargetName="configuratorPlaceholder">
                            <EasingDoubleKeyFrame KeyTime="0:0:0" Value="275"/>
                            <EasingDoubleKeyFrame KeyTime="0:0:0" Value="120">
                                <EasingDoubleKeyFrame.EasingFunction>
                                    <CircleEase />
                                </EasingDoubleKeyFrame.EasingFunction>
                            </EasingDoubleKeyFrame>
                        </DoubleAnimationUsingKeyFrames>
                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Visibility)" Storyboard.TargetName="configuratorPlaceholder">
                            <DiscreteObjectKeyFrame KeyTime="0">
                                <DiscreteObjectKeyFrame.Value>
                                    <Visibility>Visible</Visibility>
                                </DiscreteObjectKeyFrame.Value>
                            </DiscreteObjectKeyFrame>
                            <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                <DiscreteObjectKeyFrame.Value>
                                    <Visibility>Collapsed</Visibility>
                                </DiscreteObjectKeyFrame.Value>
                            </DiscreteObjectKeyFrame>
                        </ObjectAnimationUsingKeyFrames>
                    </Storyboard>
                </VisualState>
                <VisualState x:Name="ConfiguratorOpened">
                    <Storyboard>
                        <DoubleAnimationUsingKeyFrames EnableDependentAnimation="True" Storyboard.TargetProperty="(FrameworkElement.Width)" Storyboard.TargetName="configuratorPlaceholder">
                            <EasingDoubleKeyFrame KeyTime="0" Value="120"/>
                            <EasingDoubleKeyFrame KeyTime="0:0:0" Value="300">
                                <EasingDoubleKeyFrame.EasingFunction>
                                    <CircleEase/>
                                </EasingDoubleKeyFrame.EasingFunction>
                            </EasingDoubleKeyFrame>
                        </DoubleAnimationUsingKeyFrames>
                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Visibility)" Storyboard.TargetName="configuratorPlaceholder">
                            <DiscreteObjectKeyFrame KeyTime="0">
                                <DiscreteObjectKeyFrame.Value>
                                    <Visibility>Collapsed</Visibility>
                                </DiscreteObjectKeyFrame.Value>
                            </DiscreteObjectKeyFrame>
                            <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                <DiscreteObjectKeyFrame.Value>
                                    <Visibility>Visible</Visibility>
                                </DiscreteObjectKeyFrame.Value>
                            </DiscreteObjectKeyFrame>
                        </ObjectAnimationUsingKeyFrames>
                    </Storyboard>
                </VisualState>
                <VisualState x:Name="ConfiguratorOpenedOnTop">
                    <Storyboard>
                        <DoubleAnimationUsingKeyFrames EnableDependentAnimation="True" Storyboard.TargetProperty="(FrameworkElement.Width)" Storyboard.TargetName="configuratorPlaceholder">
                            <EasingDoubleKeyFrame KeyTime="0" Value="120"/>
                            <EasingDoubleKeyFrame KeyTime="0:0:0.0" Value="300">
                                <EasingDoubleKeyFrame.EasingFunction>
                                    <CircleEase/>
                                </EasingDoubleKeyFrame.EasingFunction>
                            </EasingDoubleKeyFrame>
                        </DoubleAnimationUsingKeyFrames>
                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Visibility)" Storyboard.TargetName="configuratorPlaceholder">
                            <DiscreteObjectKeyFrame KeyTime="0">
                                <DiscreteObjectKeyFrame.Value>
                                    <Visibility>Collapsed</Visibility>
                                </DiscreteObjectKeyFrame.Value>
                            </DiscreteObjectKeyFrame>
                            <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                <DiscreteObjectKeyFrame.Value>
                                    <Visibility>Visible</Visibility>
                                </DiscreteObjectKeyFrame.Value>
                            </DiscreteObjectKeyFrame>
                        </ObjectAnimationUsingKeyFrames>
                    </Storyboard>
                </VisualState>
            </VisualStateGroup>
            <VisualStateGroup x:Name="VisualStateGroup">
                <VisualState x:Name="VisualStateLarge">
                    <VisualState.StateTriggers>
                        <AdaptiveTrigger MinWindowWidth="780"/>
                    </VisualState.StateTriggers>
                </VisualState>
                <VisualState x:Name="VisualStateMedium">
                    <VisualState.StateTriggers>
                        <AdaptiveTrigger MinWindowWidth="720"/>
                    </VisualState.StateTriggers>
                    <VisualState.Setters>
                        <Setter Target="prevButton.(UIElement.Visibility)" Value="Visible"/>
                        <Setter Target="nextButton.(UIElement.Visibility)" Value="Visible"/>
                    </VisualState.Setters>
                </VisualState>
                <VisualState x:Name="VisualStateSmall">
                    <VisualState.StateTriggers>
                        <AdaptiveTrigger MinWindowWidth="0"/>
                    </VisualState.StateTriggers>
                    <VisualState.Setters>
                        <Setter Target="prevButton.(UIElement.Visibility)" Value="Collapsed"/>
                        <Setter Target="nextButton.(UIElement.Visibility)" Value="Collapsed"/>
                        <Setter Target="rootPivot.IsLocked" Value="True"/>
                        <Setter Target="demoHeader.(UIElement.Visibility)" Value="Collapsed"/>
                        <Setter Target="codeHeader.(UIElement.Visibility)" Value="Collapsed"/>
                        <Setter Target="example.Margin" Value="0 0 0 10"/>
                        <Setter Target="examplePivotItem.(Control.Padding)">
                            <Setter.Value>
                                <Thickness>0</Thickness>
                            </Setter.Value>
                        </Setter>
                        <Setter Target="codePivotItem.(Control.Padding)">
                            <Setter.Value>
                                <Thickness>0</Thickness>
                            </Setter.Value>
                        </Setter>
                        <Setter Target="rootPivot.(FrameworkElement.Margin)">
                            <Setter.Value>
                                <Thickness>0</Thickness>
                            </Setter.Value>
                        </Setter>
                    </VisualState.Setters>
                </VisualState>
            </VisualStateGroup>
        </VisualStateManager.VisualStateGroups>
    </Grid>
</common:LayoutAwarePage>
